<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.hogedriven.client.S3Client">
   <children>
      <HBox>
         <children>
            <VBox>
               <children>
                  <HBox>
                     <children>
                        <Button mnemonicParsing="false" onAction="#getBuckets" text="listup" />
                        <Button mnemonicParsing="false" onAction="#deleteBucket" text="delete" />
                     </children>
                  </HBox>
                  <ListView fx:id="bucketList" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <GridPane>
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="158.0" minWidth="10.0" prefWidth="86.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="267.0" minWidth="10.0" prefWidth="172.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="206.0" minWidth="10.0" prefWidth="71.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button mnemonicParsing="false" onAction="#uploadFile" text="upload" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                  <Button mnemonicParsing="false" onAction="#createBucket" text="Create" GridPane.columnIndex="2" />
                  <Label text="Bucket" />
                  <TextField fx:id="bucketName" GridPane.columnIndex="1" />
                  <TextField fx:id="keyName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="Key" GridPane.rowIndex="1" />
                  <Label text="FileName" GridPane.rowIndex="2" />
                  <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <children>
                        <Label text="select file" />
                        <Button mnemonicParsing="false" onAction="#chooseFile" text="select" />
                     </children>
                  </HBox>
               </children>
            </GridPane>
         </children>
      </HBox>
      <Label fx:id="status" text="status bar" />
   </children>
</VBox>
