<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.hogedriven.client.S3Client">
   <children>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" />
          <ColumnConstraints hgrow="SOMETIMES" />
            <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="bucketName" />
            <HBox GridPane.rowIndex="1">
               <children>
                  <Button mnemonicParsing="false" onAction="#getBuckets" text="List" />
                  <Button mnemonicParsing="false" onAction="#createBucket" text="Add" />
                  <Button mnemonicParsing="false" onAction="#deleteBucket" text="Delete" />
               </children>
            </HBox>
            <TextField fx:id="keyName" GridPane.columnIndex="1" />
            <HBox GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Button mnemonicParsing="false" onAction="#chooseFile" text="Select" />
                  <Button mnemonicParsing="false" onAction="#uploadFile" text="Upload" />
                  <Button mnemonicParsing="false" onAction="#deleteFile" text="Delete" />
               </children>
            </HBox>
            <Label fx:id="selectedFile" text="(file name)" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <ListView fx:id="bucketList" GridPane.rowIndex="3" />
            <ListView fx:id="objectList" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
</Pane>
