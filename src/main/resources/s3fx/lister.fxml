<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="s3fx.ListerController">
   <children>
      <StackPane>
         <children>
            <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0">
               <children>
                  <VBox>
                     <children>
                        <TextField fx:id="bucket1" />
                        <TableView fx:id="bucket1Table" minWidth="-Infinity" prefWidth="250.0">
                          <columns>
                            <TableColumn fx:id="bucket1Name" minWidth="-Infinity" prefWidth="145.0" text="name" />
                            <TableColumn fx:id="bucket1Time" minWidth="-Infinity" prefWidth="100.0" text="time" />
                          </columns>
                        </TableView>
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <TextField fx:id="bucket2" />
                        <TableView fx:id="bucket2Table" minWidth="-Infinity" prefWidth="250.0">
                          <columns>
                            <TableColumn fx:id="bucket2Name" minWidth="-Infinity" prefWidth="145.0" text="name" />
                            <TableColumn fx:id="bucket2Time" minWidth="-Infinity" prefWidth="100.0" text="time" />
                          </columns>
                        </TableView>
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <TextField fx:id="bucket3" />
                        <TableView fx:id="bucket3Table" minWidth="-Infinity" prefWidth="250.0">
                          <columns>
                            <TableColumn fx:id="bucket3Name" minWidth="-Infinity" prefWidth="145.0" text="name" />
                            <TableColumn fx:id="bucket3Time" minWidth="-Infinity" prefWidth="100.0" text="time" />
                          </columns>
                        </TableView>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <ProgressIndicator fx:id="indicator" visible="false" />
         </children>
      </StackPane>
      <Button defaultButton="true" mnemonicParsing="false" onAction="#refresh" prefWidth="700.0" text="こうしん">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </Button>
   </children>
</VBox>
